<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "BulletMLI.dtd">
<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
  <action label="top">
    <repeat>
      <times>10</times>
      <action>
        <fire>
          <direction type="sequence">10</direction>
          <bullet />
        </fire>
        <wait>1</wait>
      </action>
    </repeat>
  </action>
</bulletml>