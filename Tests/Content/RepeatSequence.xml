<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "BulletMLI.dtd">
<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
  <action label="top">
    <repeat>
      <times>10</times>
      <action>
        <changeSpeed>
          <speed type="relative">1</speed>
          <term>0</term>
        </changeSpeed>
        <wait>1</wait>
      </action>
    </repeat>
  </action>
</bulletml>